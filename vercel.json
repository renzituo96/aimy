{
  "version": 2,
  "builds": [
    {
      "src": "server/backend.js",
      "use": "@vercel/node"
    },
    {
      "src": "(?!server).*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/backend.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}