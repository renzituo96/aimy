{
  "version": 2,
  "builds": [
    {
      "src": "server/backend.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "server/backend.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["sfo1", "fra1", "hnd1"],
  "memory": 1024
}